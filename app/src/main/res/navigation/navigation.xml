<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="org.d3if2041.hitungkecepatan.ui.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_kecepatanFragment4"
            app:destination="@id/kecepatanFragment4" />
        <action
            android:id="@+id/action_homeFragment_to_balokFragment"
            app:destination="@id/balokFragment" />
    </fragment>
    <fragment
        android:id="@+id/kecepatanFragment4"
        android:name="org.d3if2041.hitungkecepatan.ui.KecepatanFragment"
        android:label="@string/kecepatan_header"
        tools:layout="@layout/fragment_kecepatan" />
    <fragment
        android:id="@+id/balokFragment"
        android:name="org.d3if2041.hitungkecepatan.ui.BalokFragment"
        android:label="@string/balok_header"
        tools:layout="@layout/fragment_balok" />
</navigation>